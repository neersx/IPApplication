{
    "name": "inprotech",
    "author": "",
    "license": "Proprietary",
    "description": "inprotech condor project",
    "version": "1.0.0",
    "main": "client/condor/app.js",
    "private": true,
    "jest": {
        "preset": "jest-preset-angular",
        "roots": [
            "<rootDir>/client/ng",
            "<rootDir>/client/signin"
        ],
        "globals": {
            "__TRANSFORM_HTML__": true
        },
        "testPathIgnorePatterns": [
            "<rootDir>[/\\\\](coverage|dist|docs|node_modules|config)[/\\\\]"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/client/ng/setupJest.ts",
        "testResultsProcessor": "jest-teamcity-reporter",
        "transform": {
            "^.+\\.(ts|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js",
            "^.+\\.js$": "babel-jest"
        },
        "testMatch": [
            "**/!(mock)/?(*.)+(spec|test).+(ts|js)?(x)"
        ],
        "testURL": "http://localhost/",
        "coverageDirectory": "coverage/ngCoverage",
        "moduleDirectories": [
            "client/ng",
            "node_modules"
        ],
        "snapshotSerializers": [
            "jest-preset-angular/AngularSnapshotSerializer.js",
            "jest-preset-angular/HTMLCommentSerializer.js"
        ],
        "verbose": false
    },
    "scripts": {
        "clearnode": "echo \"Clear node_modules \" && rimraf node_modules && rmdir /Q /S node_modules",
        "lint:ng": "ng lint --format=verbose",
        "start": "gulp serve --realapi",
        "serve": "gulp serve",
        "test": "gulp test",
        "combineCoverage": "istanbul-combine-updated -d coverage -p summary -r lcov -r html  coverage/**/coverage-final.json",
        "build": "gulp build --",
        "build:ng": "npm run _buildCondorNg -- --aot=true && npm run _delDevIndex && npm run _buildSigninNg -- --aot=true",
        "build:ng:watch": "npm run _buildSigninNg && npm run _buildCondorNg -- --watch",
        "build:ng:prod": "npm run _buildCondorNg:prod && npm run _delNgIndex && npm run _buildSigninNg:prod",
        "build:ng:hybrid": "npm run _buildSigninNg:prod && ng build condorNg --configuration=hybrid --vendorSourceMap=false --deploy-url=ng/ --output-path=dist\\ng --watch",
        "build:ng:prod:compressNg": "npm run _gzipNg && npm run _delNgJs",
        "build:ng:prod:e2e": "npm run _buildCondorNg:prod -- --configuration=e2e && npm run _delNgIndex && npm run _buildSigninNg:prod",
        "jest": "jest --",
        "jest:watch": "jest --watchAll --",
        "test:log": "echo \"Test result output into testresult.log\" && gulp test --nolint --nobuild > testresult.log 2>&1",
        "empty": "rimraf --",
        "clearSourceMaps": "rimraf dist\\ng\\vendor.js.map  dist\\ng\\runtime.js.map dist\\ng\\polyfills.js.map dist\\ng\\styles.js.map dist\\ng\\index.html && echo \"removed sourceMaps\"",
        "_buildCondorNg": "ng build condorNg --deploy-url=tmp/ng/",
        "_buildCondorNg:prod": "ng build condorNg --prod --deploy-url=ng/ --output-path=dist\\ng",
        "_buildSigninNg": "ng build signinNg",
        "_buildSigninNg:prod": "ng build signinNg --prod --output-path=dist\\signin && move ./dist/redirect ./dist/signin/redirect ",
        "_delNgIndex": "del dist\\ng\\index.html",
        "_gzipNg": "gzip dist\\ng\\*.js",
        "_gzipCondor": "gzip dist\\condor\\*.js",
        "_delNgJs": "del dist\\ng\\*.js",
        "_delDevIndex": "del client\\tmp\\ng\\index.html",
        "report": "webpack-bundle-analyzer client/tmp/ng/stats.json",
        "report:ng": "webpack-bundle-analyzer dist/ng/stats.json",
        "doc": "npx compodoc -p ./tsconfig.json"
    },
