<?xml version="1.0" encoding="utf-8" ?>
<appSettings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!-- in seconds-->
  <add key="MonitorInterval" value="5" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <!--ParentPath is case sensitive-->
  <add key="ParentPath" value="CPAInpro" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>  
	<!-- updated by Inprotech.Setup -->
	<add key="IntegrationServerBaseUrl" value="http://localhost/inprotech-integration-server-defaultinstance/" xdt:Transform="Insert"/>
  <add key="StorageServiceBaseUrl" value="http://localhost/inprotech-storage-service-defaultinstance/" xdt:Transform="Insert"/>

  <add key="AuthenticationMode" value="Forms,Windows" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  <add key="cpa.sso.clientId" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  <add key="cpa.sso.clientSecret" value="" xdt:Transform="Replace" xdt:Locator="Match(key)"/>

  <add key="cpa.sso.iamUrl" value="https://users.sso.ipplatform.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="cpa.sso.serverUrl" value="https://sso.ipplatform.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="cpa.sso.certificate" value="" xdt:Transform="Replace" xdt:Locator="Match(key)"/>

  <add key="cpa.iam.proxy.server" value="Custom" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="cpa.iam.proxy.serverUrl" value="https://users.sso.ipplatform.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>

  <add key="cpa.ipp.url" value="https://www.ipplatform.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  <add key="cpa.api.url" value="https://api.ipplatform.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
</appSettings>