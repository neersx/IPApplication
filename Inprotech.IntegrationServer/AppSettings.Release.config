<?xml version="1.0" encoding="utf-8"?>

<appSettings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<add key="ClockInterval" value=".5" xdt:Transform="Insert" /><!-- in minutes-->
	<add key="TsdrBaseUrl" value="https://tsdrapi.uspto.gov/" xdt:Transform="Insert" />
	<add key="TsdrBaseDocUrl" value="https://tsdrsec.uspto.gov/ts/cd/casedoc/" xdt:Transform="Insert" />
	<add key="EpoAuthUrl" value="https://ops.epo.org/3.2/auth/accesstoken" xdt:Transform="Insert" />
