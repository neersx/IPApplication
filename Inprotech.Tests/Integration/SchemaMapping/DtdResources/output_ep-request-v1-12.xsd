 <element name='fee-sub-amount' type='string'>
 </element>

 <element name='fee-reduction-factor'>
  <complexType>
  <simpleContent>
  <extension base='string'>
   <attribute name='user-input' type='string'/>
   </extension>
   </simpleContent>
  </complexType>
 </element>

 <element name='fee-reduction-amount'>
  <complexType>
  <simpleContent>
  <extension base='string'>
   <attribute name='user-input' type='string'/>
   </extension>
   </simpleContent>
  </complexType>
 </element>

 <element name='other-fee'>
  <complexType>
   <sequence>
    <element minOccurs='0' maxOccurs='unbounded' ref='t:fee'/>
   </sequence>
  </complexType>
 </element>

 <element name='additional-fee'>
  <complexType>
   <sequence>
    <element minOccurs='0' maxOccurs='unbounded' ref='t:fee'/>
   </sequence>
  </complexType>
 </element>

 <element name='fee-calc-info'>
  <complexType>
   <sequence>
    <element ref='t:search-region'/>
    <element ref='t:IPEA'/>
    <element ref='t:IPER'/>
   </sequence>
  </complexType>
 </element>

 <element name='search-region'>
  <complexType>
   <attribute name='region' default="EP">
    <simpleType>
     <restriction base='string'>
      <enumeration value='Other'/>
      <enumeration value='EP'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <element name='IPEA'>
  <complexType>
   <attribute name='ipea' default="EP">
    <simpleType>
     <restriction base='string'>
      <enumeration value='EP'/>
      <enumeration value='other'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <element name='IPER'>
  <complexType>
   <attribute name='iper-type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='rational'/>
      <enumeration value='detailed'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <element name='fee-total-amount' type='string'>
 </element>

 <element name='ep-notes-to-EPO'>
  <complexType>
   <sequence>
    <element minOccurs='0' maxOccurs='1' ref='t:author'/>
    <element minOccurs='0' maxOccurs='1' ref='t:subject'/>
    <element ref='t:date'/>
    <element ref='t:dtext'/>
   </sequence>
   <attribute name='id' type='ID'/>
   <attribute name='page' type='string' use='required'/>
  </complexType>
 </element>

 <element name='author' type='string'>
 </element>

 <element name='subject' type='string'>
 </element>

 <element name='doc-page'>
  <complexType>
   <attribute name='id' type='ID'/>
   <attribute name='he' type='NMTOKEN' use='required'/>
   <attribute name='wi' type='NMTOKEN' use='required'/>
   <attribute name='type' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='jpg'/>
      <enumeration value='pdf'/>
      <enumeration value='tif'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='alt' type='string'/>
   <attribute name='pp' type='string'/>
   <attribute name='ppf' type='string'/>
   <attribute name='ppl' type='string'/>
   <attribute name='file' type='string' use='required'/>
   <attribute name='color'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='color'/>
      <enumeration value='bw'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='orientation' default="portrait">
    <simpleType>
     <restriction base='string'>
      <enumeration value='portrait'/>
      <enumeration value='landscape'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>
</schema>
